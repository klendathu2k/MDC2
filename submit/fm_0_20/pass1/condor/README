condor_q | grep ' H ' | grep hepmc | grep sHijing_0_20fm > bla
for i in `cat bla | awk '{print $12}' | awk -F- '{print $3}' | awk -F. '{print "log/condor-0000000040-"$1".job"}'`; do sed -i 's/7GB/8184MB/' $i; echo $i; done
#change base prio
for i in `cat bla | awk '{print $12}' | awk -F- '{print $3}' | awk -F. '{print "log/condor-0000000040-"$1".job"}'`; do sed -i 's/=[[:space:]]20/= 22/' $i; echo $i; done
for i in `cat bla| awk '{print $1}'`; do condor_rm $i; done
 for i in `cat bla | awk '{print $12}' | awk -F- '{print $3}' | awk -F. '{print "log/condor-0000000040-"$1".job"}'`; do condor_submit $i; done
